            "FindPattern":"<c>([^<]*)</c>",
            "Replacement":"`$1`"
         },
         {
            "FindPattern":"<param[^\\>]*>([^<]*)</param>",
            "Replacement":""
         },
         {
            "FindPattern":"<returns>([^<]*)</returns>",
            "Replacement":""
         },
         {
            "FindPattern":"<see[ ]*cref=",
            "Replacement":"see `$1`"
         },
         {
            "FindPattern":"(</?example>|</?summary>|</?remarks>)",
            "Replacement":""
         }
      ]
   }
]